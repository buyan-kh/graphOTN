{
  "name": "@gotn/server",
  "version": "0.1.0",
  "description": "GoTN MCP Server",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "build:cloud": "tsc src/cloudMain.ts --outDir dist --target es2022 --module es2022 --moduleResolution node --esModuleInterop true --allowSyntheticDefaultImports true --skipLibCheck true",
    "dev": "tsc --watch --preserveWatchOutput",
    "dev:cloud": "tsx watch src/cloudMain.ts",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "start:cloud": "node dist/cloudMain.js"
  },
  "dependencies": {
    "@gotn/core": "workspace:*",
    "@gotn/vec": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.17.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}
